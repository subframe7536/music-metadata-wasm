{
    "name": "music-metadata-wasm",
    "type": "module",
    "version": "0.0.1",
    "description": "read / write the music file metadata using wasm, powered by lofty-rs",
    "author": "subframe7536",
    "license": "AGPL-3.0-only",
    "keywords": [
        "metadata",
        "wasm",
        "wasm-pack",
        "music"
